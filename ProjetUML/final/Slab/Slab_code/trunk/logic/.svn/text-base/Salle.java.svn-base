package logic;

import java.util.ArrayList;

public class Salle {

    private String numSalle;
    private ArrayList<Caracteristique> caracteristiques;
    
    /**
     * constructeur de la classe
     * @param numSalle
     */
    public Salle(String numSalle) {
    	this.numSalle=numSalle;
    }
    
    /**
     * 
     * @return le num de la salle
     */
    public String getNumSalle()
    {
    	return numSalle;
    }
    
    /**
     * modifie la salle avec la salle en paramètre
     * @param numSalle
     */
    public void setNumSalle(String numSalle)
    {
    	this.numSalle=numSalle;
    }
    
    /**
     * 
     * @return les caractéristiques
     */
    public ArrayList<Caracteristique> getCarateristique()
    {
    	return this.caracteristiques;
    }
    
    /**
     * modifie la liste des caractéristiques avec la liste des caractéristiques en paramètre
     * @param caracteristiques
     */
    public void setCaracteristique(ArrayList<Caracteristique> caracteristiques)
    {
    	this.caracteristiques=caracteristiques;
    }
    
    /**
     * @return affichage de l'objet
     */
    public String toString() {
    	
    	return this.numSalle;
    }
}