package logic;

import java.util.ArrayList;

/**
 * permet la gestion des salles 
 * @author 
 *
 */
public abstract class GestionnaireSalle {

	private ArrayList<Salle>  mySalles = new ArrayList<Salle>();
	
	/**
	 * constructeur de la classe
	 */
	public GestionnaireSalle(){
		
	}
	
	/**
	 * retourne une liste de salles disponibles pour une réservation donnée
	 * @param res reservation
	 * @return la listes des salles disponibles
	 */
	public abstract ArrayList<Salle> getSallesDisponible(Seance res);
}
